          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        },
        "type": "timeseries",
        "title": "Request Rate by Adapter",
        "targets": [
          {
            "expr": "rate(adapter_request_total[5m])",
            "refId": "A",
            "legendFormat": "{{adapter}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "never",
              "stacking": {
                "mode": "normal",
                "group": "A"
              }
            },
            "unit": "reqps",
            "min": 0
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
