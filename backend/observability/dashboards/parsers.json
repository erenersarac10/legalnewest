        "id": 5,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        },
        "type": "timeseries",
        "title": "Circuit Breaker Status",
        "targets": [
          {
            "expr": "adapter_circuit_state",
            "refId": "A",
            "legendFormat": "{{adapter}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "lineWidth": 3,
              "fillOpacity": 40,
              "showPoints": "always",
              "pointSize": 6
            },
            "unit": "none",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.5,
                  "color": "yellow"
                },
                {
                  "value": 1,
                  "color": "red"
                }
              ]
            },
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "CLOSED",
                    "color": "green"
                  },
                  "0.5": {
                    "text": "HALF-OPEN",
                    "color": "yellow"
                  },
                  "1": {
                    "text": "OPEN",
                    "color": "red"
                  }
                }
              }
            ]
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        },
        "description": "0=Closed (healthy), 0.5=Half-open (testing), 1=Open (failing)"
      },
