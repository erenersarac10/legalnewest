{
  "dashboard": {
    "id": null,
    "uid": "parser-error-rates",
    "title": "Parser Error Rates - Adapter Health Matrix",
    "tags": ["parsers", "adapters", "errors", "reliability"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "adapter_circuit_state > 0",
          "iconColor": "red",
          "name": "Circuit Breaker Events",
          "tagKeys": "circuit_breaker",
          "titleFormat": "Circuit Open: {{adapter}}"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 3,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "type": "row",
        "title": "üéØ Error Rate SLO Targets (<0.5%)"
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 3
        },
        "type": "timeseries",
        "title": "Error Rate by Adapter (SLO: <0.5%)",
        "targets": [
          {
            "expr": "adapter_error_rate{adapter=\"resmi_gazete\"}",
            "refId": "A",
            "legendFormat": "Resmi Gazete"
          },
          {
            "expr": "adapter_error_rate{adapter=\"mevzuat_gov\"}",
            "refId": "B",
            "legendFormat": "Mevzuat.gov"
          },
          {
            "expr": "adapter_error_rate{adapter=\"yargitay\"}",
            "refId": "C",
            "legendFormat": "Yargƒ±tay"
          },
          {
            "expr": "adapter_error_rate{adapter=\"danistay\"}",
            "refId": "D",
            "legendFormat": "Danƒ±≈ütay"
          },
          {
            "expr": "adapter_error_rate{adapter=\"aym\"}",
            "refId": "E",
            "legendFormat": "AYM"
          },
          {
            "expr": "0.005",
            "refId": "F",
            "legendFormat": "SLO Target (0.5%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "auto"
            },
            "unit": "percentunit",
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.005,
                  "color": "yellow"
                },
                {
                  "value": 0.01,
                  "color": "red"
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "SLO Target (0.5%)"
              },
              "properties": [
                {
                  "id": "custom.lineStyle",
                  "value": {
                    "dash": [10, 10],
                    "fill": "dash"
                  }
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "orange"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "values": ["value", "min", "max"]
          }
        }
      },
      {
        "id": 3,
        "gridPos": {
          "h": 3,
          "w": 24,
          "x": 0,
          "y": 11
        },
        "type": "row",
        "title": "üìä Request Volume & Circuit Breaker Status"
      },
      {
